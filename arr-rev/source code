#include <stdio.h>
void reversearray(int arr[],int n)
{
    int i;
    printf("\nreversed array\n");
    for(i=n-1;i>=0;i--)
    {
    printf("%d",arr[i]);
    }
}

int main()
{
 int arr[5]={1,2,3,4,5};
 int n=5;
 int i;
 printf("array:\n");
 for(i=0;i<n;i++)
 {
 printf("%d",arr[i]);
 }
 reversearray(arr,n);
}